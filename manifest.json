{
  "manifest_version": 3,
  "name": "Vocabo",
  "version": "1.0.0",
  "description": "Chrome extension providing ...",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.js-gxaZrYoC.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/content.js-gxaZrYoC.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
